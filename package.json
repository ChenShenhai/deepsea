{
  "name": "deepsea",
  "version": "0.0.1",
  "description": "a simple blog",
  "main": "index.js",
  "scripts": {
    "start": "npm run build_static & npm run start_server",
    "dev": "npm run dev_server & npm run dev_static",
    "dev_debug": "./node_modules/.bin/nodemon --inspect --experimental-modules --loader ./server/esm-loader.mjs ./server/app.mjs",
    "dev_server": "./node_modules/.bin/nodemon --experimental-modules  --loader ./server/esm-loader.mjs ./server/app.mjs",
    "dev_static": "./node_modules/.bin/webpack --watch --config ./static/build/webpack.dev.config.js",
    "build_static": "./node_modules/.bin/webpack --config ./static/build/webpack.prod.config.js",
    "init_sql": "node --experimental-modules --loader ./esm-loader.mjs ./init/index.mjs",
    "start_server": "node ./server/app",
    "precommit": "./node_modules/.bin/eslint --ext .jsx,.js,.mjs ./",
    "prepush": "./node_modules/.bin/eslint --ext .jsx,.js,.mjs ./",
    "eslint_fix": "./node_modules/.bin/eslint --fix  --ext .jsx,.js,.mjs ./"
  },
  "keywords": [
    "blog"
  ],
  "author": "chenshenhai",
  "license": "MIT",
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.6.0",
    "ejs": "^2.5.5",
    "highlight.js": "^9.12.0",
    "koa": "^2.4.1",
    "koa-convert": "^1.2.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa-send": "^3.3.0",
    "koa-static": "^4.0.2",
    "koa-views": "^5.2.0",
    "marked": "^0.3.7",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "mz": "^2.6.0",
    "node-schedule": "^1.2.5",
    "request-promise": "^4.2.2",
    "reqwest": "^2.0.5",
    "resolve-path": "^1.3.3",
    "sequelize": "^4.25.1",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.13.3",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "nodemon": "^1.11.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.2",
    "webpack": "^3.10.0",
    "webpack-merge": "^3.0.0"
  },
  "engines": {
    "node": ">= 9.2.0",
    "npm": ">= 5.5.0"
  }
}
